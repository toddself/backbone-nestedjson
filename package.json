{
  "name": "backbone-nestedjson",
  "description": "A toJSON replacement that handles nested objects",
  "version": "0.1.0",
  "repository": {
    "url": "git://github.com/toddself/backbone-nestedjson.git"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "if [[ -d .git ]]; then cp $HOME/.pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit; fi",
    "pre-commit": "npm run lint && npm test",
    "lint": "jshint",
    "test": "if [[ -d test ]]; then tap test/*js; else tap *test.js; fi"
  },
  "dependencies": {},
  "devDependencies": {
    "tap": "~0.4.4",
    "backbone": "~1.1.0"
  },
  "author": {
    "name": "Todd Kennedy",
    "email": "<todd@selfassembled.org>",
    "url": "http://tck.io"
  },
  "keywords": [
    "backbone",
    "json",
    "nested models"
  ],
  "license": {
    "type": "MIT",
    "url": "https://github.com/toddself/backbone-nestedjson/blog/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  }
}
